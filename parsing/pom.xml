              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/scalavelocitytemplate</outputDirectory>
            </configuration>
          </execution>
          
          <execution>
            <id>antlr4-python3-freemarker-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/python3</sourceDirectory>
              <includes>
                <include>Python3FreemarkerTemplate.g4</include>
