              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/java8velocitytemplate</outputDirectory>
            </configuration>
          </execution>
          <execution>
            <id>antlr4-java-stringtemplate-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/java8StringTemplateTemplate</sourceDirectory>
              <includes>
                <include>Java8StringTemplateTemplate.g4</include>
              </includes>
              <arguments>
                <argument>-package</argument>
                <argument>org.antlr.parser.java8stringtemplatetemplate</argument>
              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/java8stringtemplatetemplate</outputDirectory>
            </configuration>
          </execution>
          <execution>
            <id>antlr4-java-stringtemplate-preprocessed-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/java8StringTemplatePreprocessedTemplate</sourceDirectory>
