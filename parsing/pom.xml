              </includes>
              <arguments>
                <argument>-package</argument>
                <argument>org.antlr.parser.simplevelocity</argument>
              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/simplevelocity</outputDirectory>
            </configuration>
          </execution>
          <execution>
            <id>antlr4-simpleStringTemplate-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/simpleStringTemplate</sourceDirectory>
              <includes>
                <include>SimpleStringTemplate.g4</include>
