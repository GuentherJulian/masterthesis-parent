              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/python3</outputDirectory>
            </configuration>
          </execution>
          <execution>
            <id>antlr4-csharp-freemarker-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/csharp</sourceDirectory>
              <includes>
                <include>CSharpParser.g4</include>
                <include>CSharpLexer.g4</include>
              </includes>
              <arguments>
                <argument>-package</argument>
                <argument>org.antlr.parser.csharp</argument>
              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/csharp</outputDirectory>
            </configuration>
          </execution>
          <execution>
            <id>antlr4-csharp-combined-freemarker-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/csharpCombined</sourceDirectory>
              <includes>
