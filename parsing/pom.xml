            </configuration>
          </execution>
          <execution>
            <id>antlr4-java-velocity-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/java8VelocityTemplate</sourceDirectory>
              <includes>
                <include>Java8VelocityTemplate.g4</include>
              </includes>
              <arguments>
                <argument>-package</argument>
