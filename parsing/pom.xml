          </execution>
          <execution>
            <id>antlr4-c-velocity-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/cVelocityTemplate</sourceDirectory>
              <includes>
                <include>CVelocityTemplate.g4</include>
              </includes>
              <arguments>
                <argument>-package</argument>
