              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/cvelocitytemplate</outputDirectory>
            </configuration>
          </execution>
          
          <execution>
            <id>antlr4-scala-freemarker-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/scalaFreemarkerTemplate</sourceDirectory>
              <includes>
                <include>ScalaFreemarkerTemplate.g4</include>
              </includes>
              <arguments>
                <argument>-package</argument>
                <argument>org.antlr.parser.scalafreemarkertemplate</argument>
              </arguments>
              <outputDirectory>${project.build.directory}/generated-sources/antlr4/org/antlr/parser/scalafreemarkertemplate</outputDirectory>
            </configuration>
          </execution>
          <execution>
            <id>antlr4-scala-velocity-template-parser</id>
            <goals>
              <goal>antlr4</goal>
            </goals>
            <configuration>
              <sourceDirectory>${basedir}/src/main/resources/grammars/scalaVelocityTemplate</sourceDirectory>
              <includes>
